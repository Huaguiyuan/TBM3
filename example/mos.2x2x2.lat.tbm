#Parameters
isCalculateMu	= 1
isCalculateVar 	= 1
isCalculateLDOS	= 0
isCalculateBand	= 1
isCalculateHam 	= 0

disable_quantum = 0

spin		= "on"
space		= "normal"
Nb			= 2,2,2
var_dt		= 0.01
spin_diff	= 0.00001
den_diff	= 0.01
den_mix		= 0.2
bondRadius	= 6

Mu			= 0
Temperature	= 0.1
Jh			=-1
Jse			= 0.0
Jdm			= 0.0
OOhop		= 0.3
alpha		= 0.2
B			=-0.1

#CoreCharge
VC	> 0
Bi	> 3
LS	> 2.7
Fe	> 5
Mn	> 4
O	> 0

#Init
Fe		cspin		> 0,0,1
Fe		den			> 2
Mn		cspin		> 0,0,1
Mn		den			> 0.7
O		den			> 2

%Fe:Fe:+1+0+0	1:1:pairS	> 1
%Fe:Fe:+0+1+0	1:1:pairS	> 1
%Fe:Fe:+0+0+1	1:1:pairS	> 1
%Fe:Fe:-1+0+0	1:1:pairS	> 1
%Fe:Fe:+0-1+0	1:1:pairS	> 1
%Fe:Fe:+0+0-1	1:1:pairS	> 1
%
%Fe:Fe:+1+0+0	2:2:pairS	> 1
%Fe:Fe:+0+1+0	2:2:pairS	> 1
%Fe:Fe:+0+0+1	2:2:pairS	> 1
%Fe:Fe:-1+0+0	2:2:pairS	> 1
%Fe:Fe:+0-1+0	2:2:pairS	> 1
%Fe:Fe:+0+0-1	2:2:pairS	> 1

#Hamiltonian
hundSpin  > Fe 1 > @:cspin * Jh 
hundSpin  > Fe 2 > @:cspin * Jh 

fieldB	> Fe   > [0,0,1] * B	% The B field to the classical spin
fieldB	> Fe 1 > [0,0,1] * B	% The B field to the itinerant dx2-y2 electron
fieldB	> Fe 2 > [0,0,1] * B	% The B field to the itinerant dz2 electron
fieldB	> O  1 > [0,0,1] * B	% The B field to the oxygen p electron

superEx	> Fe:Fe:+1+0+0 > @:cspin * Jse
superEx	> Fe:Fe:-1+0+0 > @:cspin * Jse
superEx	> Fe:Fe:+0+1+0 > @:cspin * Jse
superEx	> Fe:Fe:+0-1+0 > @:cspin * Jse
superEx	> Fe:Fe:+0+0+1 > @:cspin * Jse
superEx	> Fe:Fe:+0+0-1 > @:cspin * Jse

dmEx	> Fe:Fe:+1+0+0 > @:cspin * Jdm
dmEx	> Fe:Fe:-1+0+0 > @:cspin * Jdm
dmEx	> Fe:Fe:+0+1+0 > @:cspin * Jdm
dmEx	> Fe:Fe:+0-1+0 > @:cspin * Jdm
dmEx	> Fe:Fe:+0+0+1 > @:cspin * Jdm
dmEx	> Fe:Fe:+0+0-1 > @:cspin * Jdm

% Fe-O bond hopping terms
orbital	> Fe 1 > 0
orbital	> Fe 2 > 0

%screenCoulomb > Fe ~2 > @:den * alpha
%screenCoulomb > O  ~2 > @:den * alpha

hoppingHc > Fe:O:+1+0+0# 1:px >  0.5  
hoppingHc > Fe:O:+0+1+0# 1:py >  0.5  
hoppingHc > Fe:O:+0+0+1# 1:pz > -1.0  
hoppingHc > Fe:O:-1+0+0# 1:px > -0.5  
hoppingHc > Fe:O:+0-1+0# 1:py > -0.5  
hoppingHc > Fe:O:+0+0-1# 1:pz >  1.0  

hoppingHc > Fe:O:+1+0+0# 2:px > -0.866025
hoppingHc > Fe:O:+0+1+0# 2:py >  0.866025
hoppingHc > Fe:O:+0+0+1# 2:pz >  0.0
hoppingHc > Fe:O:-1+0+0# 2:px >  0.866025
hoppingHc > Fe:O:+0-1+0# 2:py > -0.866025
hoppingHc > Fe:O:+0+0-1# 2:pz > -0.0

% O-O bond hopping terms
orbital > O 1	> -1
hopping > O:O:+1+1+0# 1:1 >  1 * OOhop
hopping > O:O:+1-1+0# 1:1 > -1 * OOhop
hopping > O:O:-1+1+0# 1:1 > -1 * OOhop
hopping > O:O:-1-1+0# 1:1 >  1 * OOhop
hopping > O:O:+1+0+1# 1:1 >  1 * OOhop
hopping > O:O:+1+0-1# 1:1 > -1 * OOhop
hopping > O:O:-1+0+1# 1:1 > -1 * OOhop
hopping > O:O:-1+0-1# 1:1 >  1 * OOhop
hopping > O:O:+0+1+1# 1:1 >  1 * OOhop
hopping > O:O:+0+1-1# 1:1 > -1 * OOhop
hopping > O:O:+0-1+1# 1:1 > -1 * OOhop
hopping > O:O:+0-1-1# 1:1 >  1 * OOhop

pairingS > Fe 1:1 > 0.3
pairingS > Fe 2:2 > 0.3
